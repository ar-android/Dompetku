<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.ahmadrosid.dompetku">

    <application
        android:name=".DompetkuApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".main.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".EditTransactionActivity"
            android:label="@string/activity_edit_transaction">
        </activity>
        <activity android:name=".detail.DetailTransactionActivity">
        </activity>

        <meta-data android:name="AA_DB_NAME" android:value="DompetkuDB" />
        <meta-data android:name="AA_DB_VERSION" android:value="7" />

        <meta-data
            android:name="AA_MODELS"
            android:value="com.ahmadrosid.dompetku.models.Transaction" />

    </application>

</manifest>